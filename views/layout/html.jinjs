<!doctype html>
<html class="no-js" lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

    <title>{% block header_title%}{% endblock %}</title>
    <meta name="description" content="{% block header_title %}{% endblock %}">
    <meta name="author" content="{% block header_author %}{% endblock %}">

    <meta name="viewport" content="{% block header_viewport %}width=device-width{% endblock %}">
    
    {% block css_styles %}
    {% endblock %}
</head>
<body>
    <div id="page-wrapper">
        <header>
        {% block page_header %}
        {% endblock %}
        </header>
        
        <div role="main" id="content">
        {% block content %}
        {% endblock %}
        </div>
    </div>
    <footer>
        {% block page_footer%}
        {% endblock %}
        </footer>
{% block javascripts %}
{% endblock %}
</body>
</html>